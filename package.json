{
    "name": "rush",
    "preview": true,
    "publisher": "rush-lang",
    "displayName": "Rush",
    "description": "Rush Programming Language Support for Visual Studio Code",
    "version": "0.0.1",
    "license": "MIT",
    "icon": "icon.png",
    "scripts": {
        "package": "vsce package -o ./rush-0.0.1.vsix",
        "install-dev-extension": "npm run package && code --install-extension ./rush-0.0.1.vsix"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/rush-lang/vscode-rush.git"
    },
    "bugs": {
        "url": "https://github.com/rush-lang/vscode-rush/issues"
    },
    "engines": {
        "vscode": "^1.56.0"
    },
    "categories": [
        "Programming Languages",
        "Debuggers",
        "Linters",
        "Formatters",
        "Other"
    ],
    "keywords": [
        "rush",
        "rush-lang",
        "programming",
        "language"
    ],
    "contributes": {
        "languages": [{
            "id": "rush",
            "aliases": ["Rush", "rush"],
            "extensions": [".rush"],
            "configuration": "./language-configuration.json"
        }, {
            "id": "rush.markdown",
            "aliases": ["Markdown", "markdown"]
        }],
        "grammars": [{
            "language": "rush",
            "scopeName": "source.rush",
            "path": "./syntaxes/rush.tmLanguage.json"
        }, {
            "language": "rush.markdown",
            "scopeName": "markdown.rush.codeblock",
            "path": "./syntaxes/markdown.tmLanguage.json",
            "injectTo": [ "text.html.markdown" ],
            "embeddedLanguages": {
                "meta.embedded.block.rush": "rush"
            }
        }],
        "snippets": [{
            "language": "rush",
            "path": "./snippets.json"
        }]
    }
}