{
  "name": "rush",
  "publisher": "rush-lang",
  "displayName": "Rush",
  "description": "Rush Programming Language Support for Visual Studio Code",
  "version": "0.0.0-semantic-release",
  "license": "MIT",
  "icon": "icons/extension-icon.png",
  "readme": "README.md",
  "main": "./dist/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/rush-lang/vscode-rush.git"
  },
  "bugs": {
    "url": "https://github.com/rush-lang/vscode-rush/issues"
  },
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Programming Languages",
    "Debuggers",
    "Linters",
    "Formatters",
    "Other"
  ],
  "keywords": [
    "rush",
    "rush-lang",
    "programming",
    "language"
  ],
  "activationEvents": [
    "onLanguage:rush"
  ],
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits"
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits"
        }
      ],
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "semantic-release-vsce",
        {
          "packageVsix": true
        }
      ],
      [
        "@semantic-release/github",
        {
          "successComment": "This issue has been resolved in ${nextRelease.version}.",
          "assets": [
            {
              "path": "*.vsix",
              "label": "Extension File"
            }
          ]
        }
      ]
    ]
  },
  "contributes": {
    "breakpoints": [
      {
        "language": "rush"
      }
    ],
    "languages": [
      {
        "id": "rush",
        "aliases": [
          "Rush",
          "rush"
        ],
        "extensions": [
          ".rush"
        ],
        "icon": {
          "dark": "./icons/file-icon.svg",
          "light": "./icons/file-icon.svg"
        },
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "rush",
        "scopeName": "source.rush",
        "path": "./syntaxes/rush.tmLanguage.json",
        "embeddedLanguages": {
          "meta.embedded.block.yaml": "yaml",
          "meta.embedded.block.json": "json",
          "meta.embedded.block.html": "html",
          "meta.embedded.block.scss": "scss",
          "meta.embedded.block.xml": "xml",
          "meta.embedded.block.css": "css",
          "meta.embedded.block.sql": "sql"
        }
      },
      {
        "scopeName": "markdown.rush.codeblock",
        "path": "./syntaxes/markdown.tmLanguage.json",
        "injectTo": [
          "text.html.markdown"
        ],
        "embeddedLanguages": {
          "meta.embedded.block.rush": "rush"
        }
      }
    ],
    "snippets": [
      {
        "language": "rush",
        "path": "./snippets.json"
      }
    ],
    "configurationDefaults": {
      "[rush]": {
        "editor.tabSize": 3,
        "editor.wordBasedSuggestions": false,
        "editor.suggest.insertMode": "replace",
        "editor.semanticHighlighting.enabled": true
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "publish": "semantic-release",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p -w . --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "install-dev-extension": "vsce package -o ./dist/rush-dev.vsix --pre-release && code --install-extension ./dist/rush-dev.vsix --force"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/github": "^8.0.4",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.61.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "@vscode/test-electron": "^1.6.2",
    "conventional-changelog-conventionalcommits": "^4.6.3",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.1",
    "semantic-release": "^19.0.2",
    "semantic-release-vsce": "^5.2.0",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.3",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.8.0"
  }
}
